let a = 0 in 
    let b = add1(a) in 
        let c = add1(b) in 
            let d = add1(c) in 
                let e = add1(d) in 
                    let f = add1(e) in 
                        let g = add1(f) in 
                            let h = add1(g) in 
                                let i = add1(h) in 
                                    a * b * c * d * e * f * g * h * i
